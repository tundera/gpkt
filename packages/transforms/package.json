{
  "name": "@gpkt/transforms",
  "version": "0.1.8",
  "license": "MIT",
  "author": {
    "name": "Philip Johnston",
    "url": "https://github.com/tundera",
    "email": "tunderadev@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/gpkt",
    "directory": "packages/transforms"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts-resolve",
    "typecheck": "tsc --noEmit",
    "lint": "TIMING=1 eslint --ext .ts,.tsx,.js,.jsx --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@mrleebo/prisma-ast": "0.4.0",
    "@prisma/sdk": "3.15.2",
    "jscodeshift": "0.13.1",
    "recast": "0.21.2"
  },
  "devDependencies": {
    "@gpkt/tsconfig": "workspace:*",
    "@types/eslint": "8.4.5",
    "@types/jest": "28.1.8",
    "@types/jscodeshift": "0.11.5",
    "@types/node": "18.7.1",
    "ast-types": "0.14.2",
    "c8": "7.12.0",
    "eslint": "8.21.0",
    "eslint-config-gpkt": "workspace:*",
    "filedirname": "2.7.0",
    "jest": "28.1.3",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "tsup": "6.2.1",
    "typescript": "4.9.5",
    "vite": "3.0.5",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.21.1"
  }
}
